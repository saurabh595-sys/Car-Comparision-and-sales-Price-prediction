	z-index: 1;
    left:10%
}
#logo {
	text-align: center;
	color: #fff;
	font-size: 50px;
	margin-top: 0%;
    position:absolute;
}

@keyframes line-anim{
    to{
        stroke-dashoffset:0;
    }
}
@keyframes fill{
    from{
        fill:transparent;
    }
    to{
        fill:white;
    }
}
@keyframes animate {
	0% {
		transform: translateY(0) rotate(0deg);
		opacity: 1;
	}
	100% {
		transform: translateY(-800px) rotate(360deg);
		opacity: 0;
	}
}
@keyframes move {
	0% {
		transform: translate3d(-200%, 0, 0)
	}
	60% {
		transform: translate3d(100%, 0, 0)
	}
	100% {
		transform: translate3d(100%, 0, 0)
	}
}
